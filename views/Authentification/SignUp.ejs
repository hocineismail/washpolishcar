        document.getElementById("checboxCity").style.display = "block";

        document.getElementById("valuecheckboxcountry").checked = false;
        document.getElementById("valuecheckboxcity").checked = false;
       
        clickcountry = false
        clickzoneValue = false
        clickcity = false
     } else {
